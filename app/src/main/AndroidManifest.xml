<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.phoneapphw">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PhoneAppHW">
        <activity android:name=".PhonesInfoActivity"></activity>
        <activity android:name=".PhonesListActivity" />
        <activity android:name=".AddPhoneActivity" />
        <activity android:name=".PhoneCategoryActivity" />
        <activity android:name=".PhonesActivity" />
        <activity android:name=".AddActivity" />
        <activity android:name=".Info" />
        <activity android:name=".SamsungActivity" />
        <activity android:name=".iPhoneActivity" />
        <activity android:name=".PhoneActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>